<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Directors History"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <directormovies
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Director Movies"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <directorname
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Provide Director Name"
                                                fieldLabel="Director Name"
                                                name="./directorname"/>
                                            <directormovies
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                fieldDescription="Provide Director Movies"
                                                fieldLabel="Director Movie">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    name="./directormovies"/>
                                            </directormovies>
                                            <moviesdetails
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldDescription="Provide Director Movie Details"
                                                fieldLabel="Movie Details">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./moviesdetails">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <moviename
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Provide Movie Name"
                                                            fieldLabel="Movie Name"
                                                            name="./moviename"/>
                                                        <movieplot
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Provide Movie Plot"
                                                            fieldLabel="Movie Plot"
                                                            name="./movieplot"/>
                                                        <releasedyear
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Provide Released Year"
                                                            fieldLabel="Released Year"
                                                            name="./releasedyear"/>
                                                    </items>
                                                </field>
                                            </moviesdetails>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </directormovies>
                    <moviesbudget
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Movies Budget"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <moviesdetailsbudget
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldDescription="Provide Director Movie Details"
                                                fieldLabel="Movie Details">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./moviesdetailsbudget">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <moviename
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Provide Movie Name"
                                                            fieldLabel="Movie Name"
                                                            name="./moviename"/>
                                                        <moviereleasedate
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                            fieldDescription="Provide Movie Released Date"
                                                            fieldLabel="Released Date"
                                                            name="./releasedate"/>
                                                        <moviebudget
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                            fieldDescription="Provide Movie Budget"
                                                            fieldLabel="Movie Budget"
                                                            name="./moviebudget"/>
                                                    </items>
                                                </field>
                                            </moviesdetailsbudget>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </moviesbudget>
                    <moviesequels
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Movies Sequels"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <moviessequeldetails
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldDescription="Provide Director Movie Details"
                                                fieldLabel="Movie Details">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./moviessequeldetails">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <moviename
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Provide Movie Name"
                                                            fieldLabel="Movie Name"
                                                            name="./moviename"/>
                                                        <movieplot
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Provide Movie Plot"
                                                            fieldLabel="Movie Plot"
                                                            name="./movieplot"/>
                                                        <moviesequels
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                            composite="{Boolean}true"
                                                            fieldDescription="Provide Movies Sequels Details"
                                                            fieldLabel="Movie Sequels">
                                                            <field
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                name="./moviesequels">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <movietitle
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Provide Movie Sequel Title"
                                                                        fieldLabel="Movie Title"
                                                                        name="./movietitle"/>
                                                                    <releasetime
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                                        fieldDescription="Provide Movie Sequel Release Time"
                                                                        fieldLabel="Release Time"
                                                                        name="./releasetime"/>
                                                                </items>
                                                            </field>
                                                        </moviesequels>
                                                    </items>
                                                </field>
                                            </moviessequeldetails>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </moviesequels>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
